        ;

    ASB_lat on                                      ! Control variables
        YAGE (c1)
        PAGE (c2)
        SIB (c3)
        INC (c4)
        ;

    ASB_lat on SES (e4);                            ! SES affects X, M and Y?
    PMH_lat on SES (e1);
    CON_lat on SES (e2);
    COH_lat on SES (e3);

    CON_lat with COH_lat (f1);                      ! Family conditions covary


