            ;

! Structural part

    CON_lat on PMH_lat (a1);                        ! X -> M1
    COH_lat on PMH_lat (a2);                        ! X -> M2

    ASB_lat on
        CON_lat (b1)                                ! M1 -> Y
        COH_lat (b2)                                ! M2 -> Y
