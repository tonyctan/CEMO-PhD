BASIC
LET CENTIME = 1
IF TIMEO = 10 THEN LET CENTIME = 0
RUN
SURVIVAL
GRAPH

MODEL TIMEO  / CENSOR=CENTIME STRATA=GROUP
ESTIMATE 
LTAB
